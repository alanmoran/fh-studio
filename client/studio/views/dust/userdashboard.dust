<script>
    function showHide(id){
      $('#apiDocs').hide();
      if (id=="clientLink"){
        $('#cloudLink').show('fast');
        $('#client').show('fast');
        $('#cloud').hide('fast');
      } 
      else {
        $('#clientLink').show('fast');
        $('#cloud').show('fast');
        $('#client').hide('fast');
      }
      return false;
    }

    function hideApis(){
      $('#apiDocs').show('fast');
      $('#clientLink').show('fast');
      $('#cloudLink').show('fast');
      $('#client').hide('fast');
      $('#cloud').hide('fast');
      return false;
    }
</script>

