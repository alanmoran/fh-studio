<div class="sidebar">
           <%- partial( "sidebar" ) %>
</div>
<div class="content">
  
  <h2>Sign Up</h2>
  <form>
    <fieldset>
    <legend>Enter some details</legend>
      <div class="clearfix">
        <label for="username">Username</label>
        <div class="input">
          <input class="xlarge" id="username" name="username" size="30" type="text">
        </div>
      </div>
      <div class="clearfix">
        <label for="password">Password</label>
        <div class="input">
          <input class="xlarge" id="password" name="password" size="30" type="password">
        </div>
      </div>
      <div class="clearfix">
        <label for="email">email</label>
        <div class="input">
          <input class="xlarge" id="email" name="email" size="30" type="email">
        </div>
      </div>
    </fieldset>
  </form>
  
  <%- partial("footer") %>
</div>